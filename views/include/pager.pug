
if paginate.hasPreviousPages || paginate.hasNextPages(pageCount)

	ul.pagination.justify-content-center
	
		a(href=paginate.href({page:1})).page-link 처음
	
		if paginate.hasPreviousPages
			li.page-item
				a(href=paginate.href(true)).page-link.prev 이전
				
		if pages
			each page,index in pages
				p= page.page
				if currentPage == page.number
					li.page-item.active
						a(href=page.url).page-link= page.number
				else
					li.page-item
						a(href=page.url).page-link= page.number

					
		if paginate.hasNextPages(pageCount)
			li.page-item
				a(href=paginate.href()).page-link.next 다음
				
				
		a(href=paginate.href({page:pageCount})).page-link 끝